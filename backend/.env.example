# Servidor
PORT=9002
NODE_ENV=development
FRONTEND_URL=http://localhost:5173

# Banco (local via infra/docker-compose.yml)
# senha "dev@2026" precisa estar URL-encoded ("@" => "%40")
DATABASE_URL="postgresql://app_user:dev%402026@localhost:5432/app_db?schema=public"

# OAuth Google (crie em Google Cloud Console)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_CALLBACK_URL=http://localhost:9002/auth/google/callback

# JWT (na VPS use valor forte)
JWT_SECRET=dev@2026
JWT_EXPIRES_IN=2h
